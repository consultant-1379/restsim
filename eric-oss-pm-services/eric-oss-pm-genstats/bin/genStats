            if [ ${STATS_MAX_CONCURRENT} -eq 1 ]  ; then
                processSim ${SIM}
                if [ $? -ne 0 ] ; then
                    exit 1
                fi
            else
                NUM_RUNNING=`ls ${CC_DIR} | wc -l | awk '{print $1}'`
                while [ ${NUM_RUNNING} -ge ${STATS_MAX_CONCURRENT} ] ; do
                    sleep 1
                    NUM_RUNNING=`ls ${CC_DIR} | wc -l | awk '{print $1}'`
                done
                LOCK_FILE=${CC_DIR}/${STARTDATE_UTC}.${SIM}
                touch ${LOCK_FILE}

                runConcurrent ${LOCK_FILE} ${SIM} &
            fi
        fi
    done
else
