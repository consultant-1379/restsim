            if [[ ${REPLAY_ENABLED} == "false" ]] || [[ ${EBSN_PERFORMANCE_ENABLED} == "true" ]]; then
                log "WARNING : Skipping Replay for ${NF} as Replay is set to ${REPLAY_ENABLED} in /netsim/netsim_cfg..."
                continue
            fi
        else
            net_func_folder="${NF}"
        fi
        STATIC_REPLAY_LOC="/ericsson/pmic/REPLAY/${net_func_folder}/${epoch_rop_folder}"        # Target directory
        STATIC_TEMPLATE_DIR="/netsim_users/pms/xml_templates/replay/${NF}/"             # Source template directory

        if [[ ! -d ${STATIC_REPLAY_LOC} ]]; then
            log "WARNING: Directory ${STATIC_REPLAY_LOC} not present, creating one."
            mkdir -p ${STATIC_REPLAY_LOC}
            if [[ $? -eq 0 ]]; then
                log "INFO : Directory ${STATIC_REPLAY_LOC} created."
            else
                log "ERROR : Not able to create directory ${STATIC_REPLAY_LOC}"
                log "WARNING : Skipping generation for ${NF} for EBSN"
                continue
            fi
        fi

        log "INFO : Replaying PM ROP information for ${NF}."

        for STATIC_REPLAY_FILE in `ls ${STATIC_TEMPLATE_DIR}`; do               # iterating on source template dir
            STATIC_REPLAY_TEMPLATE="${STATIC_TEMPLATE_DIR}/${STATIC_REPLAY_FILE}"               # source template file full path
            NEW_TEMPLATE_NAME=$(echo ${STATIC_REPLAY_TEMPLATE} | awk -F'/' '{print $8}' | cut -c 30- )
            if [[ ${NF} != "ERBS" ]]; then
                NEW_TEMPLATE_NAME="A${ROP_START_DATE_LOCAL}.${ROP_START_TIME_LOCAL}${ROP_LOCAL_OFFSET}-${ROP_END_TIME_LOCAL}${ROP_LOCAL_END_OFFSET}${NEW_TEMPLATE_NAME}.gz"
            else
                NEW_TEMPLATE_NAME="A${ROP_START_DATE_LOCAL}.${ROP_START_TIME_LOCAL}${ROP_LOCAL_OFFSET}-${ROP_END_TIME_LOCAL}${ROP_LOCAL_END_OFFSET}${NEW_TEMPLATE_NAME}"
                cp ${STATIC_REPLAY_TEMPLATE} ${STATIC_REPLAY_LOC}/${NEW_TEMPLATE_NAME}
                continue
            fi
            NEW_TEMPLATE_LOCATION="${STATIC_REPLAY_LOC}/${NEW_TEMPLATE_NAME}"
            if [[ ${EBSN_REPLAY_COUNTER_MAPPING} == "true" ]]; then
                if [[ -f ${EBSN_MAPPING_JSON} ]]; then
                    ${PYTHON_ROP_GEN_SCRIPT} -n "EBSN_REPLAY" -o "${STATIC_REPLAY_TEMPLATE}" -f "${NEW_TEMPLATE_LOCATION}" -s "${EBSN_START_DATE_TIME_LOCAL}" -e "${EBSN_END_DATE_TIME_LOCAL}" -d "${ECIM_JOB_DURATION}" -m "${EBSN_MAPPING_JSON}"
                else
                    ${PYTHON_ROP_GEN_SCRIPT} -n "EBSN_REPLAY" -o "${STATIC_REPLAY_TEMPLATE}" -f "${NEW_TEMPLATE_LOCATION}" -s "${EBSN_START_DATE_TIME_LOCAL}" -e "${EBSN_END_DATE_TIME_LOCAL}" -d "${ECIM_JOB_DURATION}"
                fi
            else
                ${PYTHON_ROP_GEN_SCRIPT} -n "EBSN_REPLAY" -o "${STATIC_REPLAY_TEMPLATE}" -f "${NEW_TEMPLATE_LOCATION}" -s "${EBSN_START_DATE_TIME_LOCAL}" -e "${EBSN_END_DATE_TIME_LOCAL}" -d "${ECIM_JOB_DURATION}"
            fi
        done
